<form method="post" action="{{ path('app_survey_delete', {'id': survey.id}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ survey.id) }}">
    <button class="btn btn-danger mx-2" title="Supprimer"><i class="fa-solid fa-trash-can"></i></button>
</form>
