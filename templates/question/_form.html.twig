<div class="form pb-5">
    {{ form_start(form) }}
    <div>
        {{ form_row(form.question) }}
    </div>
    <div class="d-flex justify-content-between">
        {{ form_row(form.answer) }}
        {{ form_row(form.imgFileName) }}
    </div>
<div class="d-flex  justify-content-between">
    {{ form_row(form.choice2) }}
    {{ form_row(form.choice3) }}
    {{ form_row(form.choice4) }}
    {{ form_row(form.choice5) }}
</div>

    {#    {{ form_widget(form) }} #}
    <button class="btn btn-primary">{{ button_label|default('Valider') }}</button>
    {{ form_end(form) }}
</div>